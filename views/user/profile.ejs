<center>
    <h1>Welcome <%=user.name%></h1>
</center>
<div class="watchlistContainer">
    <div id="item1">
        <img src="/images/Avatar.png" alt="Avatar" class="avatar" width="200" height="100"><br>
        <a>change password</a>
    </div>
    <div id="item2">
        <h3>Favorite Movies</h3>
        <div>
            <% status.forEach(stat => { %>
            <div>
                <img src="<%= stat.movie.image %>" alt="movie image" width="200" height="200"><br>
                <p><%= stat.movie.name %></p>
            </div>
            <% }) %>
        </div>
    </div>
</div>